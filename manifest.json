{
    "name": "Mr.Sagasu",
    "version": "1.1",
    "description": "search bookmark in the address bar",
    "omnibox": {
        "keyword": "b"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": [
        "bookmarks",
        "storage"
    ],
    "icons": {
        "16": "images/mrsagasu_icon16.png",
        "32": "images/mrsagasu_icon32.png",
        "48": "images/mrsagasu_icon48.png",
        "128":"images/mrsagasu_icon128.png"
    },
    "manifest_version": 2
}
